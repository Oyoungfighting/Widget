<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ClearEditText">
        <attr name="clear_icon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="DotIndicatorView">
        <attr name="indicator_select_color" format="color"/>
        <attr name="indicator_unselect_color" format="color"/>
        <attr name="indicator_margin" format="dimension"/>
        <attr name="indicator_radius" format="dimension"/>
        <attr name="indicator_gravity">
            <enum name="left" value="0"/>
            <enum name="center" value="1"/>
            <enum name="right" value="2"/>
        </attr>
        <attr name="indicator_mode">
            <enum name="inside" value="0"/>
            <enum name="outside" value="1"/>
            <enum name="solo" value="2"/>
        </attr>
    </declare-styleable>
</resources>